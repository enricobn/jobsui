<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.jobsui.ui.javafx.StartController">
    <HBox fillHeight="false" layoutX="12.0" layoutY="14.0" prefWidth="178.0" VBox.vgrow="NEVER" spacing="5">
        <padding>
            <Insets top="5" right="5" bottom="5" left="5"/>
        </padding>
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
        <JFXButton mnemonicParsing="false" onAction="#onOpen" text="Open" styleClass="button-raised"/>
        <JFXButton mnemonicParsing="false" onAction="#onEdit" text="Edit" styleClass="button-raised"/>
        <JFXButton mnemonicParsing="false" onAction="#onNew" text="New" styleClass="button-raised"/>
    </HBox>
    <ListView fx:id="projects" layoutX="3.0" layoutY="5.0" prefWidth="228.0" VBox.vgrow="ALWAYS"/>
</VBox>
