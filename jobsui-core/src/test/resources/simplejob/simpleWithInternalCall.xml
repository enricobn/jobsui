<?xml version="1.0"?>
<Job name="Simple with internal call" version="1.0.0">

    <Parameter key="name" name="Name">

        <OnInit>
            component.setDefaultValue("Enrico");
        </OnInit>

    </Parameter>

    <Parameter key="surname" name="Surname"/>

    <Call key="inv" name="Inv" project="this" job="concat">
        <Map in="name" out="first"/>
        <Map in="surname" out="second"/>
    </Call>

    <Run>
        return inv
    </Run>

</Job>