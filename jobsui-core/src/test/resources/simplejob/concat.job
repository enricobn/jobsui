<?xml version="1.0"?>
<Job name="Concat">

    <Parameter key="first" name="First">

        <CreateComponent>
            def uiValue = ui.create(UIValue.class);
            uiValue.setConverter(new StringConverterString());
            return uiValue;
        </CreateComponent>

        <Validate>
            return new NotEmptyStringValidator().validate(value);
        </Validate>

    </Parameter>

    <Parameter key="second" name="Second">

        <CreateComponent>
            def uiValue = ui.create(UIValue.class);
            uiValue.setConverter(new StringConverterString());
            return uiValue;
        </CreateComponent>

        <Validate>
            return new NotEmptyStringValidator().validate(value);
        </Validate>

    </Parameter>

    <Run>
        <![CDATA[
            return first + second
        ]]>
    </Run>
</Job>