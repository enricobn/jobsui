<?xml version="1.0"?>
<Job name="Concat" version="1.0.0">

    <Parameter key="first" name="First">
        <Validate>
            <![CDATA[
                return new NotEmptyStringValidator().validate(values, value);
            ]]>
        </Validate>
    </Parameter>

    <Parameter key="second" name="Second">
        <Validate>
            <![CDATA[
                return new NotEmptyStringValidator().validate(values, value);
            ]]>
        </Validate>

    </Parameter>

    <Run>
        <![CDATA[
            return first + second
        ]]>
    </Run>
</Job>